<body class="resultlist">
<table border="1">
  <tr>
    <th>id.</th><th>score</th><th>time</th></tr>
    <% @evaluations.each do |evaluation| %>
      <tr>
        <td><%= evaluation.id %></td>
        <td><%= evaluation.score %></td> 
        <td><%= evaluation.updated_at %></td>
      </tr>
    <% end %>
</table>
</body>